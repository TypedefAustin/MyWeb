<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
    <servlet-name>MyFirstServlet</servlet-name>
    <servlet-class>com.mrlin.Servlet.registeServlet</servlet-class>
    </servlet>
    <servlet>
    <servlet-name>LoginForward</servlet-name>
    <servlet-class>com.mrlin.Servlet.loginForwardServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Ajax</servlet-name>
        <servlet-class>com.mrlin.Servlet.ajaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Searchpost</servlet-name>
        <servlet-class>com.mrlin.Servlet.searchServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletSkip</servlet-name>
        <servlet-class>com.mrlin.Servlet.skipServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>MyFirstServlet</servlet-name>
        <url-pattern>/WebContent/do</url-pattern>
        <url-pattern>/WebContent/loginDemo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginForward</servlet-name>
        <url-pattern>/WebContent/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Ajax</servlet-name>
        <url-pattern>/WebContent/Ajax01.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Searchpost</servlet-name>
        <url-pattern>/WebContent/Search.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletSkip</servlet-name>
        <url-pattern>/WebContent/Skip</url-pattern>
    </servlet-mapping>
</web-app>